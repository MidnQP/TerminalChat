{
	"name": "chatnet-socketio-client",
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"build": "tsc -p tsconfig.json && tsc-alias -p tsconfig.json",
		"build:bin": "npm run build && npm run esbuild:cjs && npm run pkg",
		"build:bin:all": "npm run build && npm run esbuild:cjs && npm run pkg:all",
		"pkg:all": "npx pkg --no-bytecode --public --options no-warnings -t node18-linux-x64,node18-macos-x64,node18-win-x64 ./dist/bundle.cjs -o ./dist/bin",
		"pkg": "npx pkg --no-bytecode --public --options no-warnings -t node18 ./dist/bundle.cjs -o ./dist/bin && cp -r ./node_modules/leveldown/prebuilds dist/",
		"esbuild:cjs": "npx esbuild dist/client.js --outfile=dist/bundle.cjs --format=cjs --platform=node",
		"webpack:esmtocjs": "webpack-cli"
	},
	"dependencies": {
		"express": "^4.18.2",
		"fs-extra": "^11.1.1",
		"level": "^8.0.0",
		"leveldown": "^6.1.1",
		"levelup": "^5.1.1"
	},
	"devDependencies": {
		"@babel/preset-env": "^7.21.4",
		"@types/fs-extra": "^11.0.1",
		"@types/leveldown": "^4.0.3",
		"@types/levelup": "^5.1.2",
		"babel-loader": "^9.1.2",
		"level-rocksdb": "^5.0.0",
		"rocksdb": "^5.2.1",
		"socket.io-client": "^4.6.1"
	},
	"peerDependencies": {
		"bufferutil": "^4.0.7",
		"utf-8-validate": "^5.0.10"
	}
}
