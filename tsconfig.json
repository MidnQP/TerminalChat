{
  "compilerOptions": {
    "outDir": "dist",
    "moduleResolution": "NodeNext",
    "module": "NodeNext",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*"
  ],
  "typeAcquisition": {
    "enable": true
  }
}